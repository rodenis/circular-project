{
  "name": "circular-project",
  "version": "0.0.1",
  "description": "CircularJS - Cycle.js with use of Kefir.js",
  "main": "dist/bundle.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "test": "./node_modules/.bin/tape -r babel-register test/**/*.js",
    "lint": "./node_modules/.bin/eslint --quiet **/**/*.js",
    "clean": "rm -rf ./build/*",
    "precommit": "npm run clean && npm run test && npm run lint",
    "prepush": "npm run clean && npm run test && npm run lint"
  },
  "keywords": [
    "FRP"
  ],
  "author": "Denis Rodin",
  "license": "MIT",
  "dependencies": {
    "incremental-dom": "^0.3.0",
    "kefir": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.5.1",
    "eslint": "^1.10.3",
    "husky": "^0.10.2",
    "rollup": "^0.25.3",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-json": "^2.0.0",
    "tape": "^4.4.0"
  }
}
